<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="dashBoardPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DashBoardController">
   <children>
      <Label alignment="CENTER" layoutY="-1.0" prefHeight="58.0" prefWidth="600.0" style="-fx-background-color: #87CEEB;" text="ThogaKade">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <JFXButton layoutX="44.0" layoutY="95.0" onAction="#customerButtonOnAction" prefHeight="37.0" prefWidth="88.0" style="-fx-background-color: wheat;" text="Customers">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton layoutX="44.0" layoutY="149.0" onAction="#itemButtonOnAction" prefHeight="37.0" prefWidth="88.0" style="-fx-background-color: wheat;" text="Items">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton layoutX="44.0" layoutY="208.0" onAction="#orderButtonOnAction" prefHeight="37.0" prefWidth="88.0" style="-fx-background-color: wheat;" text="Oders">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton layoutX="44.0" layoutY="278.0" onAction="#placeOrderButtonOnAction" prefHeight="37.0" prefWidth="99.0" style="-fx-background-color: wheat;" text="Place Oders">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <Label fx:id="lblTime" alignment="CENTER" layoutX="485.0" layoutY="10.0" prefHeight="37.0" prefWidth="115.0" text="00:00">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="lblDate" alignment="CENTER" layoutX="392.0" layoutY="10.0" prefHeight="37.0" prefWidth="115.0" text="YYYY-MM-DD">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
